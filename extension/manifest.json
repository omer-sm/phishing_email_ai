{
    "manifest_version": 3,
    "name": "Anti Phisher",
    "version": "1.0",
    "description": "Checks Gmail mails for phishing. NOTE: make sure to run server.py!",
    "permissions": ["activeTab", "scripting", "webNavigation"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  